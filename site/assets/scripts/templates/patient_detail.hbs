<div id="p_record">
  <div id="p_brief" class="p_record_block row">
    <div id="p_brief_photo" class="col-xs-4">
        <img src="assets/img/sil-batman.jpg" alt="Patient Photo" style="width:100%;">
        </div>
        <div id="p_brief_records" class="col-xs-8">
            <div id="p_brief_name_address row" class="p_block">
                <div>{{familyName}}</div>
                <div>{{givenName}}</div>
                <div>{{address}}</div>
                <div>{{city}}, {{state}}</div>
                <div>{{zip}}</div>
            </div>
            <div id="p_brief_stats">
                <div class="row">
                    <div id="p_brief_hgt" class="p_block col-xs-3">
                        <div class="p_block_subtitle">HGT</div>
                        <div id="p_brief_hgt_val" class="p_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                    <div id="p_brief_wgt" class="p_block col-xs-3">
                        <div class="p_block_subtitle">WGT</div>
                        <div id="p_brief_wgt_val" class="p_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                    <div id="p_brief_btype" class="p_block col-xs-3">
                        <div class="p_block_subtitle">BTYPE</div>
                        <div id="p_brief_btype_val" class="p_block_val p_brief_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                    <div id="p_brief_vision" class="p_block col-xs-3">
                        <div class="p_block_subtitle">VISION</div>
                        <div id="p_brief_vision_val" class="p_block_val p_brief_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                </div>
                <div class="row">
                    <div id="p_brief_dob" class="p_block col-xs-12">
                        <div class="p_block_subtitle">DOB</div>
                        <div class = "row">
                            <div id="p_brief_dob_val" class="p_block_val p_brief_block_val col-xs-6"><div class="p_incomplete">N/A</div></div>
                            <div id="p_brief_age_val" class="p_brief_block_val col-xs-6"><div class="p_incomplete">N/A</div></div>
                        </div>
                    </div>
                    <div id="p_brief_gender" class="p_block col-xs-6">
                        <div class="p_block_subtitle">GENDER</div>
                        <div id="p_brief_gender_val" class="p_block_val p_brief_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                    <div id="p_brief_language" class="p_block col-xs-6">
                        <div class="p_block_subtitle">SPOKEN LANGUAGE</div>
                        <div id="p_brief_language_val" class="p_block_val p_brief_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                    <div id="p_brief_race" class="p_block col-xs-6">
                        <div class="p_block_subtitle">RACE</div>
                        <div id="p_brief_race_val" class="p_block_val p_brief_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                    <div id="p_brief_ethnicity" class="p_block col-xs-6">
                        <div class="p_block_subtitle">ETHNICITY</div>
                        <div id="p_brief_ethnicity_val" class="p_block_val p_brief_block_val"><div class="p_incomplete">N/A</div></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <hr>
    <div id="p_allergies" class="p_record_block row">
        <!--<div class="row">
            <div class="col-xs-6"><div class="p_block_title">ALLERGIES</div></div>
            <div class="col-xs-6"><div class="p_block_subtitle">DATE RECORDED</div></div>
