<table class="table">
  <thead>
    <tr>
      <th scope="col">Code</th>
      <th scope="col">Encounter</th>
      <th scope="col">Start Date</th>
      <th scope="col">Duration</th>
    </tr>
  </thead>
  <tbody id="p_encounter_list">
    {{#each encounters}}
     <tr>
      <td>{{code}}</td>
      <td>{{name}}</td>
      <td>{{startDate}}</td>
      <td>{{duration}}</td>
    </tr>
    {{else}}
      <tr>
        <td colspan="3">No Encounters Recorded</td>
      </tr>
    {{/each}}
  </tbody>
</table>
